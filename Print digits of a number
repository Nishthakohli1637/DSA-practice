#include<bits/stdc++.h>
using namespace std;
int main(){
    int num;
    cin>>num;
    int n=num;
    int divi=1;
    while(n>=10){
        n=n/10;
        divi=divi*10;
    }
    
    while(divi>=1){
        int fd=num/divi;
        cout<<fd<<endl;
        num=num%divi;
        divi=divi/10;
        
    }
}
